<!DOCTYPE html>
<html>
    <head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>

    
    {% block stylesheets %}
        <link rel="icon" type="image/png" href="{{ asset('assets/image/tchanan.png') }}">
        <link rel="shortcut icon" href="{{ asset('assets/image/tchanan.png') }}">

        <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/font-awesome.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/bootstrap-theme.css') }}" media="screen">
        <link rel="stylesheet" href="{{ asset('css/main.css') }}">
    {% endblock %}

    {% block javascripts %}
        {{ importmap('app') }}
        <script src="jquery.min.js"></script>
        <script src="bootstrap.min.js"></script>
        <script src="{{ asset('js/headroom.min.js') }}"></script>
        <script src="{{ asset('js/jQuery.headroom.min.js') }}"></script>
        <script src="{{ asset('js/template.js') }}"></script>
    {% endblock %}
</head>
    <body>
        {% block navigation %}{% endblock %}
        
        {% block body %}{% endblock %}

        <footer>
            {% block footer %}
                {% include 'tools/footer.html.twig' %}
            {% endblock %}
        </footer>
    </body>
</html>
